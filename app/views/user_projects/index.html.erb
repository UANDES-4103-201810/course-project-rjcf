<div id = "notice" class="alert alert-success" role="alert"><%= notice %></div>

<h1>User Projects</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Proyect</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_projects.each do |user_project| %>
      <tr>
        <td><%= user_project.user_id %></td>
        <td><%= user_project.proyect_id %></td>
        <td><%= link_to 'Show', user_project %></td>
        <td><%= link_to 'Edit', edit_user_project_path(user_project) %></td>
        <td><%= link_to 'Destroy', user_project, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User Project', new_user_project_path %>
