<div id = "notice" class="alert alert-success" role="alert"><%= notice %></div>

<h1>User Fund Projects</h1>

<table>
  <thead>
    <tr>
      <th>Type</th>
      <th>User</th>
      <th>Proyect</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_fund_projects.each do |user_fund_project| %>
      <tr>
        <td><%= user_fund_project.user_id %></td>
        <td><%= user_fund_project.proyect_id %></td>
        <td><%= link_to 'Show', user_fund_project %></td>
        <td><%= link_to 'Edit', edit_user_fund_project_path(user_fund_project) %></td>
        <td><%= link_to 'Destroy', user_fund_project, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User Fund Project', new_user_fund_project_path %>
